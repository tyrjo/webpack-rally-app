{
    "extends": "airbnb/base",
    "env": {
        "browser": true,
        "jasmine": true
    },
    "globals": {
        "Ext": false,
        "Rally": false
    },
    "rules": {
        "no-underscore-dangle": "warn",
        "import/prefer-default-export": "off",
        "comma-dangle": "off",
        "arrow-spacing": "off",
        "eol-last": "off",
        "arrow-parens": "off",
        "no-param-reassign": "off",
        "padded-blocks": "off",
        "no-trailing-spaces": "off",
        "space-in-parens": "off",
        "no-unused-vars": "warn"
    }
}